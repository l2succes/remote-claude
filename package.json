{
  "name": "remote-claude-cli",
  "version": "0.1.0",
  "description": "A CLI tool for running Claude Code in GitHub Codespaces with remote task execution and notification capabilities",
  "main": "dist/index.js",
  "bin": {
    "rcli": "dist/cli.js"
  },
  "scripts": {
    "build": "tsc",
    "dev": "ts-node src/cli.ts",
    "start": "node dist/cli.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --ext .ts --fix",
    "clean": "rm -rf dist",
    "prepare": "npm run build"
  },
  "keywords": [
    "claude",
    "ai",
    "cli",
    "codespaces",
    "github",
    "automation",
    "remote",
    "notifications"
  ],
  "author": "Luc",
  "license": "MIT",
  "dependencies": {
    "commander": "^11.1.0",
    "axios": "^1.6.2",
    "express": "^4.18.2",
    "nodemailer": "^6.9.7",
    "keytar": "^7.9.0",
    "cosmiconfig": "^8.3.6",
    "chalk": "^4.1.2",
    "ora": "^5.4.1",
    "inquirer": "^8.2.6",
    "ws": "^8.14.2"
  },
  "devDependencies": {
    "@types/node": "^20.10.4",
    "@types/express": "^4.17.21",
    "@types/nodemailer": "^6.4.14",
    "@types/inquirer": "^8.2.10",
    "@types/ws": "^8.5.10",
    "@typescript-eslint/eslint-plugin": "^6.13.2",
    "@typescript-eslint/parser": "^6.13.2",
    "eslint": "^8.55.0",
    "jest": "^29.7.0",
    "@types/jest": "^29.5.8",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.3.3",
    "supertest": "^6.3.3",
    "@types/supertest": "^2.0.16"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/yourusername/remote-claude-cli.git"
  },
  "bugs": {
    "url": "https://github.com/yourusername/remote-claude-cli/issues"
  },
  "homepage": "https://github.com/yourusername/remote-claude-cli#readme"
}